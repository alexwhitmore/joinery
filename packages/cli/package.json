{
  "name": "joinery",
  "version": "0.1.0",
  "description": "Intent-driven design system with semantic layout primitives",
  "type": "module",
  "bin": {
    "joinery": "./dist/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": ["dist", "templates"],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@clack/prompts": "^0.8.0",
    "commander": "^12.0.0",
    "picocolors": "^1.1.0",
    "fs-extra": "^11.2.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/node": "^22.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": ["cli", "design-system", "layout", "css", "every-layout"],
  "license": "MIT"
}
